{
  "name": "@al-un/recitry-api",
  "version": "0.0.1",
  "description": "",
  "author": "Al-un",
  "license": "ISC",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "dev:quick-start": "DOTENV_MODE=quick-start.local nodemon src/index.ts ",
    "build": "node esbuild.mjs",
    "test": "nyc --reporter=lcov npm run test:in-memory 'src/**/*.spec.ts'",
    "test:in-memory": "DB_DIALECT=sqlite DB_STORAGE=:memory: npm run test:run",
    "test:run": "mocha -r ts-node/register -r src/mocha-root.ts --timeout 10000 --exit",
    "db:migrate": "node --loader esbuild-register/loader -r esbuild-register src/migrator.ts",
    "db:migrate:up": "node --loader esbuild-register/loader -r esbuild-register src/migrator.ts up",
    "db:seed": "node --loader esbuild-register/loader -r esbuild-register src/seeder.ts",
    "db:seed:up": "node --loader esbuild-register/loader -r esbuild-register src/seeder.ts up",
    "setup:quick-start": "node --loader esbuild-register/loader -r esbuild-register src/quick-start-init.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "bcrypt": "5.1.0",
    "cors": "2.8.5",
    "express": "4.18.2",
    "passport": "0.6.0",
    "passport-http-bearer": "1.0.1",
    "passport-local": "1.0.0",
    "pg": "8.10.0",
    "pg-hstore": "2.3.4",
    "reflect-metadata": "0.1.13",
    "sequelize": "6.29.3",
    "sequelize-typescript": "2.1.5",
    "sqlite3": "5.1.6"
  },
  "devDependencies": {
    "@al-un/recitry-core": "*",
    "@types/bcrypt": "5.0.0",
    "@types/chai": "4.3.4",
    "@types/express": "4.17.17",
    "@types/mocha": "10.0.1",
    "@types/passport": "1.0.12",
    "@types/passport-http-bearer": "1.0.37",
    "@types/passport-local": "1.0.35",
    "@types/supertest": "2.0.12",
    "aws-sdk": "2.1340.0",
    "chai": "4.3.7",
    "esbuild": "0.17.12",
    "esbuild-register": "3.4.2",
    "mocha": "10.2.0",
    "mock-aws-s3": "4.0.2",
    "nock": "13.3.0",
    "nodemon": "2.0.21",
    "supertest": "6.3.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "umzug": "3.2.1"
  }
}
